weight: bold;\n    font-family: $heading-font;\n}\n.brand{\n    font-family: $brand-font;\n}\np{\n    font-family: $main-font;\n    color: white;\n}","styles/utils/variables.scss":"// Colors\n\n$base-color: #121212;\n$dark-grey: #333333;\n$light-grey: #707070;\n$font: white;\n$akzent: #B91900;\n\n// Font\n\n$main-font: 'Open Sans';\n$heading-font: 'Lato';\n$brand-font: 'Monoton';","styles/layout/sidebar.scss":"#side_nav{\n    padding: 15px;\n    height: 100vh;\n    width: 20%;\n    background-color: black;\n\n    h4{\n        padding-bottom: 5px;\n        padding-top: 15px;\n    }\n\n    a{\n        padding-left: 15px;\n    }\n}\n","styles/layout/content.scss":"#content{\n    padding: 15px;\n    height: 100vh;\n    width: 80%;\n    background-color: $base-color;\n\n    h2{\n        margin-top: 15px;\n        margin-bottom: 5px;\n    }\n    h3{\n        margin-top: 15px;\n        margin-bottom: 5px;\n    }\n}","styles/components/moviecard.scss":".container{\n    margin-top: 20px;\n    width: 450px;\n}\n\n.moviecard{\n\n    margin-bottom: 100px;\n\n    &__background{\n    background-color: $dark-grey;\n    border-radius: 5px;\n    }\n\n    &__rank{\n        color: white;\n        padding-bottom: 10px;\n    }\n\n    &__media{\n        width: 100%;\n        border-top-left-radius: 5px;\n        border-top-right-radius: 5px;\n        cursor: pointer;\n    }\n\n    &__content{\n        padding-right: 15px;\n        padding-left: 15px;\n    }\n\n    &__time{\n        text-align: end;\n    }\n\n    &__title{\n        line-height: 1.5;\n    }\n\n    &__data{\n        margin-bottom: 20px;\n    }\n\n    &__short_description{\n        line-height: 1.5;\n\n        &--invisible{\n            display: none;\n        }\n\n        &--visible{\n            display: block;\n        }\n    }\n\n    &__long_description{\n        line-height: 1.5;   \n    }\n\n    &__extended{\n        overflow: hidden;\n\n        &--invisible{\n            transition: max-height 0.3s;\n            max-height: 0px;\n            visibility: hidden;\n        }\n        \n\n        &--visible{\n            max-height: 500px;\n            transition: max-height 0.5s;\n        }\n    }\n\n    &__directors{\n        line-height: 5;\n    }\n\n\n\n    &__folder{\n        margin-top: 25px;\n        padding-left: 15px;\n        padding-top: 15px;\n        padding-bottom: 15px;\n        border-top: solid white;\n        border-width: 0.5px;\n        border-end-start-radius: 5px;\n        border-end-end-radius: 5px;\n        cursor: pointer;\n    }\n\n\n\n    \n}","styles/components/vote.scss":".vote{\n\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    width: 150px;\n    margin-top: 20px;\n\n    &__upvote{\n        width: 27px;\n        fill: red;\n        cursor: pointer;\n\n        &--active{\n            display: none;\n        }\n    }\n\n    &__downvote{\n        width: 27px;\n        padding-top: 16px;\n        cursor: pointer;\n\n        &--active{\n            display: none;\n        }\n    }\n\n    &__number_upvotes{\n        font-size: 1.7rem;\n        font-weight: bold;\n        font-family: $heading-font;\n        margin-right: 10px;\n    }\n\n    &__number_downvotes{\n        font-size: 1.7rem;\n        font-weight: bold;\n        font-family: $heading-font;\n    }\n}","styles/main.scss":"@import 'utils/variables.scss', 'base/reset.scss', 'base/base.scss', 'base/typography.scss', 'layout/sidebar.scss', 'layout/content.scss', 'components/moviecard.scss', 'components/vote.scss';\n\n\nbody{\n    display: flex;\n    color: white;\n    background-color: $base-color;\n    font-family: $main-font,$heading-font, sans-serif;\n}\n\n\n\n\n\n"},"lineCount":null}},"error":null,"hash":"4026c9e3b5ee1a941facdc30dde43473","cacheData":{"env":{}}}