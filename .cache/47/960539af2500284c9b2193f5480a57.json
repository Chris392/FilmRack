;\n}","styles/utils/variables.scss":"// Colors\n\n$base-color: #121212;\n$dark-grey: #333333;\n$light-grey: #707070;\n$font: white;\n$akzent: #B91900;\n\n// Font\n\n$main-font: 'Open Sans';\n$heading-font: 'Lato';\n$brand-font: 'Monoton';","styles/layout/sidebar.scss":"#side_nav{\n    padding: 15px;\n    height: 100vh;\n    width: 20%;\n    background-color: black;\n\n    h4{\n        padding-bottom: 5px;\n        padding-top: 15px;\n    }\n\n    a{\n        padding-left: 15px;\n    }\n}\n","styles/layout/content.scss":".content{\n    padding: 15px;\n    height: 100vh;\n    width: 80%;\n    background-color: $base-color;\n\n    h2{\n        margin-top: 15px;\n        margin-bottom: 5px;\n    }\n    h3{\n        margin-top: 15px;\n        margin-bottom: 5px;\n    }\n}","styles/components/list_card.scss":".listcard{\n    margin: 3%;\n    width: 25%;\n    height: 155px;\n    background-image: url(\"https://m.media-amazon.com/images/M/MV5BNGNhMDIzZTUtNTBlZi00MTRlLWFjM2ItYzViMjE3YzI5MjljXkEyXkFqcGdeQXVyNzkwMjQ5NzM@._V1_SX300.jpg\");\n    background-size:cover;\n}\n.listcard__link{\n    display: block;\n    width: 100%;\n    height: 100%;\n}\n.listcard__rank{\n    display: inline-block;\n    margin-top: 30%;\n    max-height: 60%;\n    margin-left: 30%;\n    max-width: 30%;\n    font-size: 9rem;\n    font-weight: bolder;\n    font-family: $heading-font;\n    -webkit-text-fill-color: rgba($color: #000000, $alpha: 0);\n    -webkit-text-stroke: 4px white;\n    &--first{\n        margin-left: 45%;\n        font-family: $main-font;\n        -webkit-text-stroke: 3px white;\n    }\n}\n","styles/components/bottom_nav.scss":".bottom_nav{\n    width: 100%;\n    height: 67px;\n    box-shadow: 0 0 2vh rgba(0, 0, 0, 0.2);\n    background-color: $dark-grey;\n    display: flex;\n}\n.bottom_nav__link{\n    display: flex;\n    flex-grow: 1;\n    min-width: 50px;\n    overflow: hidden;\n    white-space: nowrap;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    font-size: .8rem;\n    text-decoration: none;\n    -webkit-tap-highlight-color: transparent;\n    transition: background-color 0.1s ease-in-out;\n    color: $light-grey;\n}\n.bottom_nav__link:hover {\n    background-color: grey;\n}\n.bottom_nav__link--active{\n    color: white;\n}","styles/components/event_card.scss":".event{\n    margin-top: 15px;\n    margin-bottom: 15px;\n    margin-left: 5%;\n    margin-right: 5%;\n    height: 56px;\n    width: 90%;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n}\n.event__host{\n    height: 100%;\n    border-radius: 10%;\n    img{\n        height: 100%;\n    }\n}\n.event__arrow{\n    width: 10%;\n    height: auto;\n    margin-top: 10px;\n    margin-bottom: 10px;\n    img{\n        transform: scaleX(-1);\n    }\n}\n.event__info{\n    width: 75%;\n    padding-left: 5%;\n    color: white;\n    .event__name{\n        font-size: 1.2rem;\n        margin-top: 5px;\n        margin-bottom: 6px;\n        padding: 0;\n        color: white;\n    }\n    .event__detail{\n        margin-top: 0px;\n        margin-bottom: 5px;\n        padding: 0;\n        width: 70%;\n        .event__time{\n            margin-right: 10px;\n            img{\n                height: 100%;\n                vertical-align: sub;\n            }\n        }\n        .event__date{\n            margin-right: 10px;\n            img{\n                height: 100%;\n                vertical-align: sub;\n            }\n        }\n    }\n}","styles/main.scss":"@import 'utils/variables.scss', \n'base/reset.scss',\n'base/base.scss',\n'base/typography.scss', \n'layout/sidebar.scss', \n'layout/content.scss',\n'components/list_card.scss',\n'components/bottom_nav.scss',\n'components/event_card.scss';\n\nbody{\n    display: flex;\n    color: white;\n    background-color: $dark-grey;\n    font-family: $main-font,$heading-font, sans-serif;\n}\n.mobile_component{\n    width: 450px;\n    border-color: white;\n    border: 1px solid;\n    display: flex;\n    flex-direction: row;\n}\n\n\n\n"},"lineCount":null}},"error":null,"hash":"f514afca074a0f159fc96828a35d3bbc","cacheData":{"env":{}}}